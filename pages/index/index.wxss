/* pages/one/frist.wxss */
page{
    background: #336699;
    font-family: "黑体","宋体"
    
}
.title{
    color: white;
    background: orange;
    height: 60rpx;
    line-height: 60rpx;
    font: 20px;
    text-align: center;
    box-shadow: 2px 2px 10px #B8B8B8
}
.pull-right{
    float: right;
}
.pull-left{
    float: left
}
.desc{
    padding-top: 10px;
    color: white;
    font: 16px
}
.bigCircle{
    width: 150px;
    height: 150px;
    position: absolute;
    border-radius: 50%;
}
.time{
    padding-top: 10px;
    height: 160px;
}
.right{
    clip: rect(0,150px,150px,75px)
}
.blue1{
    background: #99CCFF;
}
.blue2{
    background: #0099FF;
}
.circle{
    width: 130px;
    height: 130px;
    position: absolute;
    border-radius: 50%;
    margin-left: 10px;
    margin-top: 10px;
    background-color: white
}
.timeBtn{
    display: flex;
    flex-direction: row;
}
.btn{
    background-color: #0099CC;
    color: white;
}
.rightCircle{
    margin-left: 420rpx;
}
.circle view{
   margin-left: 50px;
   margin-top: 60px;
   color: black;
   font: 20px;
}
